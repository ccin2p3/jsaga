<?xml version="1.0" encoding="UTF-8"?>
<jsaga-default xmlns="http://www.in2p3.fr/jsaga/session">
    <contexts>
        <context type="UserPass">
            <attribute name="BaseUrlIncludes">
                <item>stest://</item>
            </attribute>
            <attribute name="DataServiceAttributes"/>
        </context>
        <context type="VOMS">
            <attribute name="UserVO" value="dteam"/>
            <attribute name="BaseUrlIncludes">
                <item>gsiftp://</item>
                <item>srm://</item>
                <item>lfn://</item>
                <item>cream://</item>
                <item>wms://</item>
            </attribute>
            <attribute name="JobServiceAttributes">
                <item>wms.virtualorganisation=dteam</item>
            </attribute>
            <attribute name="DataServiceAttributes"/>
        </context>
        <context type="Globus">
            <attribute name="BaseUrlIncludes">
                <item>gatekeeper://</item>
            </attribute>
            <attribute name="JobServiceAttributes">
                <item>gatekeeper.ShellPath=c\:\\cygwin\\bin\\sh.exe</item>
            </attribute>
        </context>
        <context type="None">
            <attribute name="BaseUrlIncludes">
                <item>local://</item>
            </attribute>
            <attribute name="JobServiceAttributes">
                <item>local.ShellPath=c\:\\cygwin\\bin\\sh.exe</item>
            </attribute>
        </context>
    </contexts>
    <session>
        <context type="UserPass">
            <attribute name="UserID" value="anonymous"/>
            <attribute name="UserPass" value="anon"/>
            <attribute name="UrlPrefix" value="STestContext"/>
        </context>
    </session>
</jsaga-default>

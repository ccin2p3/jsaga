<wsdl:definitions name="CREAM"
	targetNamespace='http://glite.org/2007/11/ce/cream'
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:common_faults="http://glite.org/ce/faults"
	xmlns:cream="http://glite.org/2007/11/ce/cream"
	xmlns:wsa="http://www.w3/org/2005/08/addressing"
	xmlns:cream_types="http://glite.org/2007/11/ce/cream/types">

	<wsdl:types>
		<xsd:schema attributeFormDefault="unqualified"
			elementFormDefault="qualified"
			xmlns:cream_types="http://glite.org/2007/11/ce/cream/types"
			xmlns:ce_faults="http://glite.org/ce/faults"
			xmlns:wsa="http://www.w3.org/2005/08/addressing"
			targetNamespace="http://glite.org/2007/11/ce/cream/types">

			<xsd:complexType name="JobId">
				<xsd:sequence>
					<xsd:element name="id" type="xsd:string" />
					<xsd:element name="creamURL" type="xsd:anyURI" />
					<xsd:element name="property" type="cream_types:Property" minOccurs="0" maxOccurs="unbounded" />
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="Property">
				<xsd:sequence>
					<xsd:element name="name" type="xsd:string" />
					<xsd:element name="value" type="xsd:string" />
				</xsd:sequence>
			</xsd:complexType>

			<xsd:element name="iceId" type="xsd:string"
				soap:mustUnderstand="true" />

			<xsd:complexType name="ServiceInfo">
				<xsd:sequence>
					<xsd:element name="interfaceVersion" type="xsd:string" />
					<xsd:element name="serviceVersion" type="xsd:string" />
					<xsd:element name="description" type="xsd:string" minOccurs="0" />
					<xsd:element name="startupTime" type="xsd:dateTime" />
					<xsd:element name="doesAcceptNewJobSubmissions" type="xsd:boolean" />
					<xsd:element name="property" type="cream_types:Property" minOccurs="0" maxOccurs="unbounded" />
					<xsd:element name="status" type="xsd:string" minOccurs="0" />
					<xsd:element name="message" type="cream_types:ServiceMessage" minOccurs="0" maxOccurs="unbounded" />
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="ServiceMessage">
				<xsd:sequence>
					<xsd:element name="type" type="xsd:string" />
					<xsd:element name="message" type="xsd:string" />
					<xsd:element name="timastamp" type="xsd:dateTime" />
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="Command">
				<xsd:sequence>
					<xsd:element name="id" type="xsd:string"
						minOccurs="0" />
					<xsd:element name="name" type="xsd:string"
						minOccurs="1" />
					<xsd:element name="category" type="xsd:string"
						minOccurs="1" />
					<xsd:element name="status" type="xsd:string"
						minOccurs="1" />
					<xsd:element name="description" type="xsd:string"
						minOccurs="0" />
					<xsd:element name="failureReason" type="xsd:string"
						minOccurs="0" />
					<xsd:element name="creationTime" type="xsd:dateTime"
						minOccurs="0" />
					<xsd:element name="startSchedulingTime" type="xsd:dateTime"
					    minOccurs="0" />
					<xsd:element name="startProcessingTime" type="xsd:dateTime"
					    minOccurs="0" />
					<xsd:element name="executionCompletedTime" type="xsd:dateTime"
					    minOccurs="0" />
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="Status">
				<xsd:sequence>
					<xsd:element name="jobId" type="cream_types:JobId"
						minOccurs="1" />
					<xsd:element name="name" type="xsd:string"
						minOccurs="1" />
					<xsd:element name="timestamp" type="xsd:dateTime"
						minOccurs="1" />
					<xsd:element name="exitCode" type="xsd:string"
						minOccurs="0" maxOccurs="1" />
					<xsd:element name="failureReason" type="xsd:string"
						minOccurs="0" />
					<xsd:element name="description" type="xsd:string"
						minOccurs="0" />
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="JobStatusResult">
				<xsd:choice>
					<xsd:element name="jobStatus" type="cream_types:Status" minOccurs="0" />
					<xsd:element ref="cream_types:JobUnknownFault" minOccurs="0" />
					<xsd:element ref="cream_types:JobStatusInvalidFault" minOccurs="0" />
					<xsd:element ref="cream_types:DelegationIdMismatchFault" minOccurs="0" />
					<xsd:element ref="cream_types:DateMismatchFault" minOccurs="0" />
					<xsd:element ref="cream_types:LeaseIdMismatchFault" minOccurs="0" />
					<xsd:element ref="cream_types:GenericFault" minOccurs="0" />
				</xsd:choice>
				<xsd:attribute name="jobDescriptionId" type="xsd:string"/>
			</xsd:complexType>

			<xsd:element name="JobStatusResponse">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="result" type="cream_types:JobStatusResult" minOccurs="0" maxOccurs="unbounded" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<xsd:complexType name="Lease">
				<xsd:sequence>
					<xsd:element name="leaseId" type="xsd:string" />
					<xsd:element name="leaseTime" type="xsd:dateTime" />
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="JobInfo">
				<xsd:sequence>
					<xsd:element name="jobId" type="cream_types:JobId" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="fatherJobId" type="cream_types:JobId" minOccurs="0" />
					<xsd:element name="childJobId" type="cream_types:JobId" minOccurs="0" maxOccurs="unbounded" />
					<xsd:element name="GridJobId" type="xsd:string" minOccurs="1" maxOccurs="1" default="N/A" />
					<xsd:element name="LRMSAbsLayerJobId" type="xsd:string" minOccurs="1" maxOccurs="1" default="N/A" />
					<xsd:element name="LRMSJobId" type="xsd:string" minOccurs="1" maxOccurs="1" default="N/A" />
					<xsd:element name="delegationProxyId" minOccurs="1"	maxOccurs="1" type="xsd:string" default="N/A" />
					<xsd:element name="delegationProxyInfo" type="xsd:string" minOccurs="1" maxOccurs="1" default="N/A" />
					<xsd:element name="workerNode" type="xsd:string" minOccurs="1" maxOccurs="1" default="N/A" />
					<xsd:element name="localUser" type="xsd:string" minOccurs="0" maxOccurs="1" default="N/A" />
					<xsd:element name="workingDirectory" type="xsd:string" minOccurs="1" maxOccurs="1" default="N/A" />
					<xsd:element name="CREAMInputSandboxURI" type="xsd:string" minOccurs="1" maxOccurs="1" default="N/A" />
					<xsd:element name="CREAMOutputSandboxURI" type="xsd:string" minOccurs="1" maxOccurs="1" default="N/A" />
					<xsd:element name="JDL" type="xsd:string" minOccurs="0" />
					<xsd:element name="type" type="xsd:string" minOccurs="1" />
					<xsd:element name="lease" type="cream_types:Lease" minOccurs="0" maxOccurs="1" />
					<xsd:element name="status" type="cream_types:Status" minOccurs="1" maxOccurs="unbounded" />
					<xsd:element name="lastCommand" type="cream_types:Command" minOccurs="0" maxOccurs="unbounded" />
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="JobInfoResult">
				<xsd:choice>
					<xsd:element name="jobInfo" type="cream_types:JobInfo" minOccurs="0" />
					<xsd:element ref="cream_types:JobUnknownFault" minOccurs="0" />
					<xsd:element ref="cream_types:JobStatusInvalidFault" minOccurs="0" />
					<xsd:element ref="cream_types:DelegationIdMismatchFault" minOccurs="0" />
					<xsd:element ref="cream_types:DateMismatchFault" minOccurs="0" />
					<xsd:element ref="cream_types:LeaseIdMismatchFault" minOccurs="0" />
					<xsd:element ref="cream_types:GenericFault" minOccurs="0" />
				</xsd:choice>
				<xsd:attribute name="jobDescriptionId" type="xsd:string"/>
			</xsd:complexType>

			<xsd:element name="JobInfoResponse">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="result" type="cream_types:JobInfoResult" minOccurs="0" maxOccurs="unbounded" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<xsd:complexType name="Event">
				<xsd:sequence>
					<xsd:element name="id" type="xsd:string" minOccurs="1" />
					<xsd:element name="type" type="xsd:string" minOccurs="1" />
					<xsd:element name="timestamp" type="xsd:dateTime" minOccurs="1" />
					<xsd:element name="property" type="cream_types:Property" minOccurs="0" maxOccurs="unbounded" />
				</xsd:sequence>
			</xsd:complexType>
			
			<xsd:element name="QueryEventResult">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="event" type="cream_types:Event" minOccurs="0" maxOccurs="unbounded" />
					   <!-- <xsd:element name="moreResults" type="xsd:boolean"/> -->
						<xsd:element name="queryExecutionTimeInMillis" type="xsd:long"/>
						<xsd:element name="dbId" type="xsd:string"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
						
			<xsd:complexType name="JobFilter">
				<xsd:sequence>
					<xsd:element name="jobId" type="cream_types:JobId" minOccurs="0" maxOccurs="unbounded" />
					<xsd:element name="status" type="xsd:string" minOccurs="0" maxOccurs="unbounded" />
					<xsd:element name="fromDate" type="xsd:dateTime" minOccurs="0" maxOccurs="1" />
					<xsd:element name="toDate" type="xsd:dateTime" minOccurs="0" maxOccurs="1" />
					<xsd:element name="delegationId" type="xsd:string" minOccurs="0" maxOccurs="1" />
					<xsd:element name="leaseId" type="xsd:string" minOccurs="0" maxOccurs="1" />
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="JobDescription">
				<xsd:sequence>
					<xsd:element name="JDL" type="xsd:string" minOccurs="1" />
					<xsd:element name="delegationId" type="xsd:string" minOccurs="0" />
					<xsd:element name="delegationProxy" type="xsd:string" minOccurs="0" />
					<xsd:element name="leaseId" type="xsd:string" minOccurs="0" />
					<xsd:element name="autoStart" type="xsd:boolean" default="true" />
				</xsd:sequence>
				<xsd:attribute name="jobDescriptionId" type="xsd:string"/>
			</xsd:complexType>

			<xsd:element name="JobRegisterRequest">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="jobDescriptionList" type="cream_types:JobDescription" minOccurs="1" maxOccurs="unbounded" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<xsd:complexType name="JobRegisterResult">
				<xsd:choice>
					<xsd:element name="jobId" type="cream_types:JobId" minOccurs="0" />
					<xsd:element ref="cream_types:DelegationProxyFault" minOccurs="0" />
					<xsd:element ref="cream_types:DelegationIdMismatchFault" minOccurs="0" />
					<xsd:element ref="cream_types:LeaseIdMismatchFault" minOccurs="0" />
					<xsd:element ref="cream_types:GenericFault" minOccurs="0" />
				</xsd:choice>
				<xsd:attribute name="jobDescriptionId" type="xsd:string"/>
			</xsd:complexType>

			<xsd:element name="JobRegisterResponse">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="result" type="cream_types:JobRegisterResult" minOccurs="1" maxOccurs="unbounded" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<xsd:complexType name="Result">
				<xsd:sequence>
					<xsd:element name="jobId" type="cream_types:JobId" minOccurs="1" maxOccurs="1" />
					<xsd:choice>				
						<xsd:element ref="cream_types:JobUnknownFault" minOccurs="0" />
						<xsd:element ref="cream_types:JobStatusInvalidFault" minOccurs="0" />
						<xsd:element ref="cream_types:DelegationIdMismatchFault" minOccurs="0" />
						<xsd:element ref="cream_types:DateMismatchFault" minOccurs="0" />
						<xsd:element ref="cream_types:LeaseIdMismatchFault" minOccurs="0" />	
						<xsd:element ref="cream_types:GenericFault" minOccurs="0" />				
					</xsd:choice>
				</xsd:sequence>
			</xsd:complexType>
			
			<xsd:complexType name="CommandResult">
				<xsd:sequence>
					<xsd:element name="result" type="cream_types:Result" minOccurs="0" maxOccurs="unbounded" />
				</xsd:sequence>
			</xsd:complexType>
			
			<xsd:element name="JobCancelRequest" type="cream_types:JobFilter" />
			<xsd:element name="JobCancelResponse" type="cream_types:CommandResult" />

			<xsd:element name="JobInfoRequest" type="cream_types:JobFilter" />

			<xsd:element name="QueryEventRequest">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="fromEventId" type="xsd:string" minOccurs="0" />
						<xsd:element name="toEventId" type="xsd:string" minOccurs="0" />
						<xsd:element name="fromDate" type="xsd:dateTime" minOccurs="0" />
						<xsd:element name="toDate" type="xsd:dateTime" minOccurs="0" />
						<xsd:element name="eventType" type="xsd:string" />
						<xsd:element name="maxQueryEventResultSize" type="xsd:int" default="100" />
						<xsd:element name="verbosityLevel" type="xsd:int" default="0" />
                                                <xsd:element name="property" type="cream_types:Property" minOccurs="0" maxOccurs="unbounded" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="setLeaseRequest" type="cream_types:Lease" />
			<xsd:element name="setLeaseResponse" type="cream_types:Lease" />

			<xsd:element name="JobSetLeaseIdRequest">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="newLeaseId" type="xsd:string" minOccurs="0" />
						<xsd:element name="jobFilter" type="cream_types:JobFilter" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="JobSetLeaseIdResponse" type="cream_types:CommandResult" />

			<xsd:element name="getLeaseRequest" type="xsd:string" />
			<xsd:element name="getLeaseResponse" type="cream_types:Lease" />
			
			<xsd:element name="deleteLeaseRequest" type="xsd:string" />

			<xsd:element name="jobLeaseRequest" type="cream_types:JobFilter" />

			<xsd:element name="getLeaseListResponse">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="result"
							type="cream_types:Lease" minOccurs="0" maxOccurs="unbounded" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>


			<!--xsd:element name="JobListRequest"  type="cream_types:JobFilter" /-->

			<xsd:element name="JobListResponse">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="result"
							type="cream_types:JobId" minOccurs="0" maxOccurs="unbounded" />
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="JobPurgeRequest" type="cream_types:JobFilter" />
			<xsd:element name="JobPurgeResponse" type="cream_types:CommandResult" />

			<xsd:element name="JobResumeRequest" type="cream_types:JobFilter" />
			<xsd:element name="JobResumeResponse" type="cream_types:CommandResult" />

			<xsd:element name="JobStartRequest" type="cream_types:JobFilter" />
			<xsd:element name="JobStartResponse" type="cream_types:CommandResult" />

			<xsd:element name="JobStatusRequest" type="cream_types:JobFilter" />

			<xsd:element name="JobSuspendRequest" type="cream_types:JobFilter" />
			<xsd:element name="JobSuspendResponse" type="cream_types:CommandResult" />

			<xsd:element name="ServiceInfoRequest" type="xsd:int" />
			<xsd:element name="ServiceInfoResponse" type="cream_types:ServiceInfo" />
			<xsd:element name="acceptNewJobSubmissions" type="xsd:boolean" />


			<xsd:complexType name="BaseFaultType">
				<xsd:sequence>
					<xsd:element name="MethodName" type="xsd:string" />
					<xsd:element name="Timestamp" type="xsd:dateTime" />
					<xsd:element name="ErrorCode" type="xsd:string"
						minOccurs="0" maxOccurs="1" />
					<xsd:element name="Description" type="xsd:string"
						minOccurs="0" maxOccurs="1" />
					<xsd:element name="FaultCause" type="xsd:string"
						minOccurs="0" maxOccurs="1" />
				</xsd:sequence>
			</xsd:complexType>

			<xsd:element name="GenericFault">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="cream_types:BaseFaultType" />
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="AuthorizationFault">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="cream_types:BaseFaultType" />
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="DelegationIdMismatchFault">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="cream_types:BaseFaultType" />
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="LeaseIdMismatchFault">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="cream_types:BaseFaultType" />
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="DateMismatchFault">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="cream_types:BaseFaultType" />
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
			
			<xsd:element name="JobUnknownFault">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="cream_types:BaseFaultType" />
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="InvalidArgumentFault">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="cream_types:BaseFaultType" />
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="OperationNotSupportedFault">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="cream_types:BaseFaultType" />
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="NoSuitableResourcesFault">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="cream_types:BaseFaultType" />
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="DelegationProxyFault">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="cream_types:BaseFaultType" />
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="JobStatusInvalidFault">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="cream_types:BaseFaultType" />
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>

			<xsd:element name="JobSubmissionDisabledFault">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="cream_types:BaseFaultType" />
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
		</xsd:schema>
	</wsdl:types>

	<!-- Messages -->

	<wsdl:message name="acceptNewJobSubmissions">
                <wsdl:part name="acceptNewJobSubmissions"
                        element="cream_types:acceptNewJobSubmissions" />
        </wsdl:message>

	<wsdl:message name="acceptNewJobSubmissionsResponse">
		<!--wsdl:part name="acceptNewJobSubmissions"
			element="cream_types:acceptNewJobSubmissionsResponse" /-->
	</wsdl:message>

	<wsdl:message name="JobCancelRequest">
		<wsdl:part name="JobCancelRequest"
			element="cream_types:JobCancelRequest" />
	</wsdl:message>

	<wsdl:message name="JobCancelResponse">
		<wsdl:part name="JobCancelResponse"
			element="cream_types:JobCancelResponse" />
	</wsdl:message>

	<wsdl:message name="JobInfoRequest">
		<wsdl:part name="JobInfoRequest"
			element="cream_types:JobInfoRequest">
		</wsdl:part>
	</wsdl:message>

	<wsdl:message name="JobInfoResponse">
		<wsdl:part name="JobInfoResponse"
			element="cream_types:JobInfoResponse">
		</wsdl:part>
	</wsdl:message>

	<wsdl:message name="QueryEventHeader">
		<wsdl:part name="iceId" element="cream_types:iceId" />
	</wsdl:message>
	
	<wsdl:message name="QueryEventRequest">
		<wsdl:part name="request"
			element="cream_types:QueryEventRequest">
		</wsdl:part>
	</wsdl:message>

	<wsdl:message name="QueryEventResponse">
		<wsdl:part name="QueryEventResult"
			element="cream_types:QueryEventResult">
		</wsdl:part>
	</wsdl:message>

	<wsdl:message name="setLeaseRequest">
		<wsdl:part name="parameters"
			element="cream_types:setLeaseRequest" />
	</wsdl:message>

	<wsdl:message name="setLeaseResponse">
		<wsdl:part name="parameters"
			element="cream_types:setLeaseResponse">
		</wsdl:part>
	</wsdl:message>

	<wsdl:message name="getLeaseRequest">
		<wsdl:part name="parameters"
			element="cream_types:getLeaseRequest">
		</wsdl:part>
	</wsdl:message>
	<wsdl:message name="getLeaseResponse">
		<wsdl:part name="parameters"
			element="cream_types:getLeaseResponse">
		</wsdl:part>
	</wsdl:message>

	<wsdl:message name="getLeaseListRequest" />
	<wsdl:message name="getLeaseListResponse">
		<wsdl:part name="parameters" element="cream_types:getLeaseListResponse">
		</wsdl:part>
	</wsdl:message>

	<wsdl:message name="deleteLeaseRequest">
		<wsdl:part name="parameters"
			element="cream_types:deleteLeaseRequest">
		</wsdl:part>
	</wsdl:message>
	<wsdl:message name="deleteLeaseResponse" />
	
	<wsdl:message name="JobSetLeaseIdRequest">
                <wsdl:part name="parameters" element="cream_types:JobSetLeaseIdRequest" />
	</wsdl:message>
	<wsdl:message name="JobSetLeaseIdResponse">

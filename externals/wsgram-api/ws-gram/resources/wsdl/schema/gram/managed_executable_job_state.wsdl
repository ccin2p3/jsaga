<wsdl:definitions
    name="ManagedExecutableJobState"
    targetNamespace=
        "http://www.globus.org/namespaces/2004/10/gram/job/exec/state"
    xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">

    <wsdl:import
        namespace="http://www.globus.org/namespaces/2004/10/gram/job/exec"
        location="../gram/managed_executable_job_port_type.wsdl" />

    <wsdl:types>
        <xsd:schema
            targetNamespace=
                "http://www.globus.org/namespaces/2004/10/gram/job/exec/state"
            xmlns:tns=
                "http://www.globus.org/namespaces/2004/10/gram/job/exec/state"
            xmlns:exec=
